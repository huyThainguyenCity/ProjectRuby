<h1>Edit Profile</h1>

<%= form_with model: @profile, local: true do |form| %>
  <%= form.search_field  :first_name %>
  <%= form.search_field :last_name %>
  <%= form.search_field :phone_number %>
  <%= form.date_field :birthday %>
  <%= form.file_field :image %><br>
  <%= form.submit %>
<% end %>
